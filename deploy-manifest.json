{
  "version": 1,
  "routes": [
    {
      "path": "/.well-known/apple-app-site-association",
      "target": {
        "kind": "Static",
        "src": "/well-known/apple-app-site-association"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Static",
        "src": "/index.html"
      }
    }
  ],
  "framework": {
    "name": "static",
    "version": "1.0"
  }
}