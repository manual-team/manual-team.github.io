{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "share.whyapp.co"
        }
      ],
      "destination": "/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/Modak.ttf"
    }
  ]
}